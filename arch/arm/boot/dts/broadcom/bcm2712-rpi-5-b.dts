	spidev1: spidev@1 {
		compatible = "spidev";
		reg = <1>;	/* CE1 */
		#address-cells = <1>;
		#size-cells = <0>;
		spi-max-frequency = <125000000>;
	};
};

// =============================================
// Downstream rpi- changes

#include "bcm2712-rpi.dtsi"

/ {
	chosen {
		bootargs = "coherent_pool=1M 8250.nr_uarts=1 snd_bcm2835.enable_compat_alsa=0 snd_bcm2835.enable_hdmi=1";
		stdout-path = "serial0";
	};

	fan: cooling_fan {
		status = "disabled";
		compatible = "pwm-fan";
		#cooling-cells = <2>;
		cooling-min-state = <0>;
		cooling-max-state = <3>;
		cooling-levels = <0 75 150 250>;
		pwms = <&rp1_pwm1 3 41566 PWM_POLARITY_INVERTED>;
	};

	suspend_button {
		compatible = "gpio-keys";

		pinctrl-names = "default";
		pinctrl-0 = <&suspend_button_pins>;
		status = "okay";

		// Define a single key, called "suspend" that monitors the gpio and sends KEY_SUSPEND
		// (keycode 205, see
		// https://github.com/torvalds/linux/blob/v4.12/include/uapi/linux/input-event-codes.h#L190)
		suspend {
			label = "suspend";
			linux,code = <205>; // KEY_SUSPEND
			interrupt-parent = <&gio>;
			interrupts = <14 IRQ_TYPE_EDGE_RISING>;
			debounce-interval = <0>; // ms
		};
	};
};

&usb {
	power-domains = <&power RPI_POWER_DOMAIN_USB>;
