// Board specific stuff here

&rp1_pwm1 {
	status = "disabled";
	pinctrl-0 = <&rp1_pwm1_gpio45>;
	pinctrl-names = "default";
};

&cpu_thermal {
	trips {
		// Add more thresholds below the point at which
		// firmware starts throttling (80C)
		cpu_hot: cpu-hot {
			temperature = <70000>;
			hysteresis = <5000>;
			type = "active";
		};

		cpu_active: cpu-active {
			temperature = <60000>;
			hysteresis = <10000>;
			type = "active";
		};

		cpu_passive: cpu-passive {
			temperature = <40000>;
			hysteresis = <10000>;
			type = "passive";
		};
	};

	cooling-maps {
		cpu-crit {
			trip = <&cpu_crit>;
			cooling-device = <&fan 3 3>;
		};

		cpu-hot {
			// Max fan starts below crit temp
			trip = <&cpu_hot>;
			cooling-device = <&fan 3 3>;
		};

		cpu-active {
			trip = <&cpu_active>;
			cooling-device = <&fan 2 2>;
		};

		cpu-passive {
			// Fan off
			trip = <&cpu_passive>;
			cooling-device = <&fan 0 0>;
		};
	};
};

